{
  "name": "@slardar/sdk-template",
  "version": "1.1.10",
  "description": "Slardar SDK Template",
  "main": "dist/index.cjs.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "esm"
  ],
  "publishConfig": {
    "registry": "http://bnpm.byted.org/"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "../../tools/ava.setup"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "../../tsconfigs/tsconfig.cjs.json"
    },
    "files": [
      "src/**/*.spec.{ts,tsx}"
    ]
  },
  "browserslist": [
    "ie >= 9",
    "Edge >= 12",
    "Firefox >= 4",
    "Opera >= 12.1",
    "and_qq >= 10.4",
    "Safari >= 5",
    "iOS >= 3.2",
    "Chrome >= 4",
    "Android >= 37",
    "ChromeAndroid >= 89",
    "UCAndroid >= 12.12"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts"
    ],
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/__tests__/**/*.*",
      "**/__test__/**/*.*",
      "**/vendors/**/*.*",
      "src/plugins/logger.ts",
      "src/utils/index.ts",
      "src/utils/logger.ts",
      "src/utils/misc.ts",
      "src/utils/uuid.ts"
    ],
    "check-coverage": false
  },
  "devDependencies": {
    "@slardar/rollup": "0.0.3-alpha.0"
  },
  "scripts": {
    "test": "ava",
    "esm": "ttsc -b ./tsconfig.json",
    "cover": "nyc --reporter=json ava && nyc report",
    "build": "batch-rollup -d ./rollup"
  },
  "gitHead": "d89afa16fccc41e454c53140d4d6b33c3c78fc62"
}
